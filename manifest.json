{
  "manifest_version": 3,
  "name": "Copy unique urls",
  "version": "1.0",
  "description": "Capture and analyze HTTP requests",
  "permissions": [
    "webRequest",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}